#Prueba t para una muestra independiente 



library (foreign)
data<-read.spss("IOPGENERO.sav", to.data.frame=TRUE, use.value.labels = TRUE)
attach(data)
library(Hmisc)

str(data)

describe(SEXO)
describe(P19A)
str(SEXO)

#Primero, prueba la normalidad de la variable escalar 
library(nortest)
lillie.test(P19A)$p.value #para pedir test de Kolmogorov-Smirnov

#Luego, se segmenta la variable nominal 
data<- transform(data,Sexo1=factor(SEXO,labels=c("Masculino","Femenino")))
attach(data)
table(Sexo1)
str(data)
library(car)
leveneTest(P19A, Sexo1, center=mean)

#Prueba t para muestras independientes 

t.test(P19A~Sexo1, alternative='two.sided', conf.level=.95,var.eqal=FALSE, data=data)